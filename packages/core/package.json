{
  "name": "@gmaps-kit/core",
  "version": "1.0.1",
  "description": "Framework-agnostic core utilities for Google Maps JavaScript SDK - geocoding, directions, places, markers, street view, and more",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "google-maps",
    "google-maps-api",
    "maps",
    "javascript",
    "typescript",
    "sdk",
    "utilities",
    "geocoding",
    "directions",
    "places",
    "markers",
    "street-view",
    "autocomplete",
    "framework-agnostic",
    "vanilla-js",
    "maps-api",
    "google-maps-sdk",
    "location",
    "coordinates",
    "latitude",
    "longitude",
    "map-utilities",
    "google-maps-utilities",
    "maps-javascript-api",
    "gmaps",
    "web-maps",
    "interactive-maps"
  ],
  "author": "gmaps-kit team",
  "license": "MIT",
  "dependencies": {
    "@types/google.maps": "^3.54.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.1.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goutham-05/gmaps-kit.git",
    "directory": "packages/core"
  }
}
